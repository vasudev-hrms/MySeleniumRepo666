var i = 0;var params = null;var listChannels = apsdb.callApi("ListChannels");for (i = 0; i < listChannels.result.channels.length; i++) {	var deleteChannel = apsdb.callApi("DeleteChannel", {		"name" : listChannels.result.channels[i].name	}, null);}var devices = apsdb.callApi("ListDevices").result.devices;for (i = 0; i < devices.length; i++) {	if (devices[i].id != "scriptr") {		var deleteDevice = apsdb.callApi("DeleteDevice", {			"id" : devices[i].id		}, null);	}}var listGroups = apsdb.callApi("ListGroups");for (i = 0; i < listGroups.result.groups.length; i++) {	var deleteGroup = apsdb.callApi("DeleteGroup", {		"groupName" : listGroups.result.groups[i].name	}, null);}var params = {	"apsdb.facebookAccessToken" : "",	"apsdb.facebookConsumerSecret" : "",	"apsdb.facebookConsumerKey" : ""};var resetFacebookConfig = apsdb.callApi("SaveConfiguration", params, null);params = {	"apsdb.twitterAccessTokenSecret" : "",	"apsdb.twitterAccessToken" : "",	"apsdb.twitterConsumerSecret" : "",	"apsdb.twitterConsumerKey" : ""};var resetTwitterConfig = apsdb.callApi("SaveConfiguration", params, null);params = {	"apsdb.gitPurge" : "",	"apsdb.defaultRepositoryPath" : "",	"apsdb.defaultRepositoryBranch" : "",	"apsdb.defaultRepositoryAccessToken" : "",	"apsdb.defaultRepositoryOwner" : "",	"apsdb.defaultRepositoryName" : ""};var resetGithubConfig = apsdb.callApi("SaveConfiguration", params, null);var resetCorsConfig = apsdb		.callApi(				"SaveConfiguration",				{					"apsdb.accessControlAllowOrigin" : "https://www.scriptr.io,https://staging.scriptr.io,https://test.scriptr.io"				}, null);var docKeys = [ "iosprod", "iosdev", "android" ];for (i = 0; i < docKeys.length; i++) {	var deleteCertificates = apsdb.callApi("DeleteDocument", {		"apsdb.documentKey" : docKeys[i],		"apsdb.store" : "SystemStore"	}, null);}var push = require("mobile-push");var listGroups = push.listGroups().result.groups;for (i = 0; i < listGroups.length; i++) {	push.deleteGroup(listGroups[i].id);}var deleteFiles = apsdb.callApi("DeleteFiles", {	"namePattern" : ".*"}, null);var jobHandle = deleteFiles.result.jobHandle;var jobStatus = "";var complete = "complete";while (jobStatus != complete) {	var getJobStatus = apsdb.callApi("GetJobStatus", {		"apsdb.handle" : jobHandle	}, null);	jobStatus = getJobStatus.result.jobStatus;	if (jobStatus == complete) {		if (getJobStatus.result.failure && getJobStatus.result.failure.length) {			return getJobStatus.result.failure;		}	}}